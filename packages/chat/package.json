    "@solana/spl-token": "0.1.8",
    "@solana/web3.js": "^1.43.4",
    "@strata-foundation/spl-token-bonding": "^3.10.3",
    "@strata-foundation/spl-utils": "^3.10.3",
    "add": "^2.0.6",
    "bn.js": "^5.2.0",
    "bs58": "^4.0.1",
    "copyfiles": "^2.4.1",
