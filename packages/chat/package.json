{
  "name": "@strata-foundation/chat",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "version": "3.9.2",
  "description": "Interface to the chat smart contract",
  "repository": {
    "type": "git",
    "url": "https://github.com/StrataFoundation/strata"
  },
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": "./dist/lib/index.js"
  },
  "files": [
    "dist/lib/**/*"
  ],
  "scripts": {
    "build": "yarn run copy-files && tsc --project tsconfig.json",
    "copy-files": "copyfiles -u 1 src/**/*.js dist/lib",
    "watch": "yarn run copy-files && tsc --watch --project tsconfig.json",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@cardinal/namespaces": "^4.1.3",
    "@cocalc/local-storage-lru": "^2.3.0",
    "@orca-so/sdk": "^1.2.24",
    "@project-serum/anchor": "^0.24.2",
    "@shadow-drive/sdk": "^3.0.4",
    "@solana/web3.js": "^1.43.4",
    "@strata-foundation/spl-utils": "^3.9.0",
    "@types/uuid": "^8.3.4",
    "add": "^2.0.6",
    "bn.js": "^5.2.0",
    "copyfiles": "^2.4.1",
    "lit-js-sdk": "^1.1.179",
    "localstorage-memory": "^1.0.3",
    "uuid": "^8.3.2",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "git-format-staged": "^2.1.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.3.4"
  },
  "gitHead": "ff670e6a6211209648c56dd69e5f5dfbda631b42"
}
