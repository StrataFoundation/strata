{
    "name": "@strata-foundation/cli",
    "description": "Bootstrapping wumbo",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/StrataFoundation/strata"
    },
    "files": [
        "dist/lib/**/*"
    ],
    "scripts": {
        "copy-files": "copyfiles -u 1 src/**/*.js dist/lib",
        "watch": "yarn run copy-files && node src/idlToTs",
        "bootstrap": "tsc && node dist/lib/bootstrap",
        "bounty-dist": "tsc && node dist/lib/bounty-dist",
        "create-governance-token": "tsc && node dist/lib/createGovernanceToken"
    },
    "dependencies": {
        "@solana/web3.js": "^1.32.0",
        "@strata-foundation/spl-token-bonding": "^3.2.5-lbc.0",
        "@strata-foundation/spl-token-collective": "^3.2.5-lbc.0",
        "@strata-foundation/spl-utils": "^3.2.5-lbc.0",
        "bn.js": "^5.2.0",
        "copyfiles": "^2.4.1",
        "semver": "^7.3.5"
    },
    "devDependencies": {
        "style-loader": "^2.0.0",
        "ts-loader": "^9.2.3",
        "typescript": "^4.3.4"
    },
    "gitHead": "32401f1dd5d6f0127c8aedbf5698c4a6f9e4001b",
    "version": "3.2.5-lbc.0"
}
