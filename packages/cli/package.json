{
    "name": "@strata-foundation/cli",
    "description": "Bootstrapping wumbo",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/StrataFoundation/strata"
    },
    "files": [
        "dist/lib/**/*"
    ],
    "scripts": {
        "copy-files": "copyfiles -u 1 src/**/*.js dist/lib",
        "watch": "yarn run copy-files && node src/idlToTs",
        "bootstrap": "tsc && node dist/lib/bootstrap",
        "create-governance-token": "tsc && node dist/lib/createGovernanceToken"
    },
    "dependencies": {
        "@solana/web3.js": "^1.32.0",
        "@strata-foundation/spl-token-bonding": "^2.2.1-alpha.4+396e6f4",
        "@strata-foundation/spl-token-collective": "^2.2.1-alpha.4+396e6f4",
        "@strata-foundation/spl-token-staking": "^2.2.1-alpha.4+396e6f4",
        "@strata-foundation/spl-utils": "^2.2.1-alpha.4+396e6f4",
        "bn.js": "^5.2.0",
        "copyfiles": "^2.4.1",
        "semver": "^7.3.5"
    },
    "devDependencies": {
        "style-loader": "^2.0.0",
        "ts-loader": "^9.2.3",
        "typescript": "^4.3.4"
    },
    "gitHead": "396e6f4df8c4ea021da409e499dba0b0136f1953",
    "version": "2.2.1-alpha.4+396e6f4"
}
