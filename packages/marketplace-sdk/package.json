{
  "name": "@strata-foundation/marketplace-sdk",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "version": "3.9.7",
  "description": "Interface to the strata marketplace",
  "repository": {
    "type": "git",
    "url": "https://github.com/StrataFoundation/strata"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
"exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.24.2",
    "@solana/web3.js": "^1.43.4",
    "@strata-foundation/fungible-entangler": "^3.9.7",
    "@strata-foundation/spl-token-bonding": "^3.9.7",
    "@strata-foundation/spl-token-collective": "^3.9.7",
    "@strata-foundation/spl-utils": "^3.9.7",
    "bn.js": "^5.2.0",
    "copyfiles": "^2.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "git-format-staged": "^2.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "vite": "^2.7.13",
    "vite-plugin-dts": "^0.9.9"
  },
  "gitHead": "ff670e6a6211209648c56dd69e5f5dfbda631b42"
}
