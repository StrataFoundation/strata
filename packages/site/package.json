    "@strata-foundation/react": "^3.5.5",
    "next": "12.0.7",
    "next-seo": "^5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-text-transition": "^1.3.0"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.10",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.7",
