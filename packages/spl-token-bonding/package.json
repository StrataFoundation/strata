{
  "name": "@strata-foundation/spl-token-bonding",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "version": "0.5.1",
  "description": "Interface to the spl-token-bonding smart contract",
  "repository": {
    "type": "git",
    "url": "https://github.com/StrataFoundation/strata"
  },
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": "./dist/lib/index.js"
  },
  "files": [
    "dist/lib/**/*"
  ],
  "scripts": {
    "build": "yarn run copy-files && tsc --project tsconfig.json",
    "copy-files": "copyfiles -u 1 src/**/*.js dist/lib",
    "watch": "yarn run copy-files && tsc --watch --project tsconfig.json",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "yarn format"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.18.0",
    "@project-serum/common": "^0.0.1-beta.3",
    "@solana/web3.js": "^1.29.2",
    "@strata-foundation/spl-utils": "^0.5.1",
    "bn.js": "^5.2.0",
    "copyfiles": "^2.4.1"
  },
  "devDependencies": {
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4"
  },
  "gitHead": "9a64fbd7484a63f4e039008a2494573e8bf99229"
}
