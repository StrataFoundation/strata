{
    "name": "@strata-foundation/spl-token-staking",
    "version": "3.1.0",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "description": "Interface to the spl-token-staking smart contract",
    "repository": {
        "type": "git",
        "url": "https://github.com/StrataFoundation/wumbo"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "exports": {
        ".": "./dist/lib/index.js"
    },
    "files": [
        "dist/lib/**/*"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "watch": "tsc --watch --project tsconfig.json",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "precommit": "git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
        "prepare": "yarn run build"
    },
    "dependencies": {
        "@solana/web3.js": "^1.32.0",
        "@strata-foundation/spl-utils": "^3.1.0",
        "bn.js": "^5.2.0",
        "semver": "^7.3.5"
    },
    "devDependencies": {
        "git-format-staged": "^2.1.3",
        "style-loader": "^2.0.0",
        "ts-loader": "^9.2.3",
        "typescript": "^4.3.4"
    },
    "gitHead": "396e6f4df8c4ea021da409e499dba0b0136f1953"
}
